{
  "name": "chat-app",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode dev",
    "build:dev": "vite build --mode dev",
    "build:olddev": "vite build --mode olddev",
    "build:prod": "vite build --mode prod",
    "lint": "eslint .",
    "preview:dev": "vite preview --mode dev",
    "preview:olddev": "vite preview --mode olddev",
    "preview:prod": "vite preview --mode prod",
    "publish": "source ./.env.dev && aws s3 sync dist/ s3://${S3_BUCKET}/ --delete && aws cloudfront create-invalidation --distribution-id ${CF_DIST} --paths \"/*\"",
    "publish:dev": "source ./.env.dev && aws s3 sync dist/ s3://${S3_BUCKET}/ --delete && aws cloudfront create-invalidation --distribution-id ${CF_DIST} --paths \"/*\"",
    "publish:prod": "source ./.env.prod && aws s3 sync dist/ s3://${S3_BUCKET}/ --delete && aws cloudfront create-invalidation --distribution-id ${CF_DIST} --paths \"/*\""
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^6.11.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "axios": "^1.11.0",
    "marked": "^16.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@tailwindcss/vite": "^4.1.14",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14",
    "vite": "^7.1.0"
  }
}
